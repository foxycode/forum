{
    "name": "foxycode/forum",
    "description": "Very simple forum written using Nette",
    "homepage": "https://forum.olmik.net",
    "type": "project",
    "license": ["MIT"],
    "authors": [
        {
            "name": "Tomas Jacik",
            "email": "tomas.jacik@sunfox.cz",
            "homepage": "http://sunfox.cz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "nette/application": "^3.0.6",
        "nette/bootstrap": "^3.0",
        "nette/caching": "^3.0",
        "nette/database": "^3.0",
        "nette/di": "^3.0",
        "nette/finder": "^2.6",
        "nette/forms": "^3.0",
        "nette/http": "^3.0",
        "nette/mail": "^3.1",
        "nette/robot-loader": "^3.4",
        "nette/security": "^3.0",
        "nette/utils": "^3.1",
        "latte/latte": "^2.11",
        "tracy/tracy": "^2.7"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "nette/tester": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        },
        "classmap": [
            "app/Bootstrap.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.1"
        }
    }
}
