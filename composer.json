        "nette/http": "^2.4",
        "nette/mail": "^2.4",
        "nette/neon": "^2.4",
        "nette/php-generator": "^2.4",
        "nette/reflection": "^2.4",
        "nette/robot-loader": "^3.0",
        "nette/safe-stream": "^2.3",
        "nette/security": "^2.4",
        "nette/tokenizer": "^2.2",
